{
  "hosting": {
    "public": "out",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/learn",
        "destination": "/learn.html"
      },
      {
        "source": "/cheatsheet",
        "destination": "/cheatsheet.html"
      }
    ],
    "redirects": [
      {
        "source": "/learn.html",
        "destination": "/learn",
        "type": 301
      },
      {
        "source": "/cheatsheet.html",
        "destination": "/cheatsheet",
        "type": 301
      }
    ]
  }
}